{
  "name": "angular-desktop-ui",
  "version": "0.4.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron:build": "webpack-cli --mode development --progress --config webpack.electron.js",
    "electron:watch": "webpack-cli --mode development --watch --progress --config webpack.electron.js",
    "electron:start": "electron .",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac"
  },
  "bin": "dist/electron/electron.js",
  "main": "dist/electron/electron.js",
  "private": true,
  "sideEffects": false,
  "dependencies": {
    "@angular/animations": "^11.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/forms": "^11.0.0",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/router": "^11.0.0",
    "@marcj/estdlib": "^0.1.17",
    "buffer": "^5.4.3",
    "core-js": "^2.5.4",
    "electron": "^9.4.0",
    "highlight.js": "^9.15.6",
    "npm-local-development": "^0.3.1",
    "rxjs": "~6.5.4",
    "showdown": "^1.9.0",
    "ts-node": "^8.0.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.1",
    "@angular/cli": "^11.0.1",
    "@angular/compiler-cli": "^11.0.0",
    "@angular/language-service": "^11.0.0",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@marcj/angular-desktop-ui": "^0.4.2",
    "@types/highlight.js": "^9.12.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/showdown": "^1.9.2",
    "babel-loader": "^8.0.4",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-loader": "^5.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.0.5",
    "webpack": "^4.29.4",
    "webpack-cli": "^3.1.2",
    "webpack-visualizer-plugin": "^0.1.11"
  }
